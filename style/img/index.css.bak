/*主内容区 幻灯片*/
.slider {
	float:left;
	width: 625px;
}
.slider-container {
	
	margin: 0 auto 10px;
	overflow: hidden;
	position: relative;
	width: 625px;
}
.slider-container .slider-wrapper {
	left: 0;
	overflow: hidden;
	position: absolute;
	top: 0;
	width: 5000em; /*Fix for prevent display errors*/
	-webkit-transition: all 800ms ease;
	-moz-transition: all 800ms ease;
	-o-transition: all 800ms ease;
	transition: all 800ms ease;
}
.slider-container .slide {
	float: left;
	height: 570px;
	overflow: hidden;
	position: relative;
	width: 625px;
}
.slider-nav {
	text-align: center;
	width: 625px;
	position:relative;
}
.slider-nav li {
	display: inline;
	padding: 3px;
}
.slider-nav .bullet {
	background-color:#CECECE;
	border-radius: 50%;
	display: inline-block;
	text-indent: -9000px;
	overflow: hidden;
	height: 14px;
	width: 14px;
}
.slider-nav .active {
	background-color: #000;
}
.slider-nav .prev, .slider-nav .next {
	background: url(http://img.chinadevelopment.com.cn/templates/chinadevelopment/css/cmstop1.8/images/ico40x40.png) no-repeat;
	text-indent: -9000px;
	overflow: hidden;
	height: 80px;
	width: 50px;
	position:absolute;
	top: -333px;
	filter: alpha(Opacity=40);
    opacity: .4;
	background-color: #000;
}
.slider-nav .prev {
	background-position: -16px -1712px;
	left: 0px;
}
.slider-nav .prev:hover,.slider-nav .next:hover {
	background-color: #000;
	filter: alpha(Opacity=80);
    opacity: .8;
}
.slider-nav .next {
	background-position: -18px -1556px;
	right: 0px;
}

